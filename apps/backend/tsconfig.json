{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "dist",
    "module": "CommonJS",
    "target": "ES2019",
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
}
